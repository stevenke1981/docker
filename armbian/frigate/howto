0. #create the dictionary and fonfig file
sudo mkdir /home/frigate
sudo nano /home/frigate/config.yml


1.#go to the dictionary
cd /home/frigate
  
2.#write something
sudo nano docker-compose.yml

3.#run frigate
cd /home/frigate
sudo docker-compose up -d

4. #restart, if needed
sudo docker-compose restart
